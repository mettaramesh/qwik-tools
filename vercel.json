{
  "version": 2,
  "builds": [
    { "src": "dist/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*\\.(js|css|wasm|woff2|ico|svg|html))$", "dest": "/dist/$1" },
    { "src": "/service-worker.js", "dest": "/dist/service-worker.js" },
    { "src": "/assets/(.*)", "dest": "/dist/assets/$1" },
    { "src": "/(.*)", "dest": "/dist/index.html" }
  ]
}
